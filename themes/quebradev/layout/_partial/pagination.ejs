<div class="pagination-buttons">
    <% if (page.prev) { %>
        <% if (page.prev === 1) { %>
            <a class="btn btn-quebradev" href="<%- url_for(' ') %>">
        <% } else { %>
                <a class="btn btn-quebradev" href="<%- url_for(page.prev_link) %>">
        <% } %>
                    <span>Página Anterior</span>
                </a>
            </a>
    <% } %>
    <% if (page.next) { %>
        <a class="btn btn-quebradev" href="<%- url_for(page.next_link) %>">
            <span>Próxima Página</span>
        </a>
    <% } %>
</div>
